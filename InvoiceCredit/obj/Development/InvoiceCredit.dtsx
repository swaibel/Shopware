          SQLTask:SqlStatementSource="Update [dbo].[RO_InvoiceCredit]&#xA;set OverUnder = InvoiceTotal - TotalPaid&#xA;&#xA;Update [dbo].[RO_InvoiceCredit]&#xA;set OverUnder = CASE&#xA;WHEN TotalPaid &gt; 0 then InvoiceTotal - TotalPaid&#xA;ELSE InvoiceTotal + TotalPaid&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
