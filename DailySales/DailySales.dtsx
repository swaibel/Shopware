      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{70B00996-976E-4CED-8497-F1185E357765}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="/****** Build the DailySales Table ******/&#xA;/* Purge table for testing*/&#xA;Use Shopware&#xA;truncate table dailysales&#xA;/* Insert the sales data */&#xA;Use Shopware&#xA;INSERT INTO [dbo].[DailySales] ([Store_id],[DrawerDate],[Parts],      [Labor],       [Hazmats],       [Warranty],[CustSat],    [Sublets],      [SalesTaxTotal],[ShopSupplies])&#xA;SELECT                          [shop_id],[ClosedDate], [Parts],      [Labor],       [Hazmats],       [Warranty],[CustomerSat],[Sublet],       [TotalTax],     [ShopSupplies]                               &#xA;FROM [Shopware].[dbo].[vw_DailySalesTotals]&#xA;  /* May need to add code to only insert the current date, unless going to purge and recreate the entire table daily*/&#xA;  go&#xA; &#xA;  /* update the Discounts */&#xA;  /* Discounts inserted here are parts &amp; labor discounts entered at the totals section of Shopware */&#xA;  update [DailySales]&#xA;  Set [DailySales].[LaborDiscount] = [vw_RODiscountTotals].[LaborDiscountTotal]&#xA;  From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.vw_RODiscountTotals ON dbo.DailySales.Store_id = dbo.vw_RODiscountTotals.shop_id AND dbo.DailySales.DrawerDate = dbo.vw_RODiscountTotals.ClosedDate&#xA;  go&#xA;  /* Update any NULLS to zero*/&#xA; Update DailySales&#xA;SET [Parts] = 0&#xA;Where [Parts] is null&#xA;go&#xA; Update DailySales&#xA;SET [Labor] = 0&#xA;Where [Labor] is null&#xA;go&#xA; Update DailySales&#xA;SET [Hazmats] = 0&#xA;Where [Hazmats] is null&#xA;go&#xA;Update DailySales&#xA;SET [Sublets] = 0&#xA;Where [Sublets] is null&#xA;go&#xA;Update DailySales&#xA;SET [LaborDiscount] = 0&#xA;Where [LaborDiscount] is null&#xA;go&#xA;Update DailySales&#xA;SET [CustSat] = 0&#xA;Where [CustSat] is null&#xA;go&#xA;Update DailySales&#xA;SET [NonTaxed] = 0&#xA;Where [NonTaxed] is null&#xA;go&#xA;Update DailySales&#xA;SET [SalesTaxTotal] = 0&#xA;Where [SalesTaxTotal] is null&#xA;go&#xA;Update DailySales&#xA;SET [SalesTaxState] = 0&#xA;Where [SalesTaxState] is null&#xA;go&#xA;Update DailySales&#xA;SET [SalesTaxCity] = 0&#xA;Where [SalesTaxCity] is null&#xA;go&#xA;Update DailySales&#xA;SET [Cash] = 0&#xA;Where [Cash] is null&#xA;go&#xA;Update DailySales&#xA;SET [Credit] = 0&#xA;Where [Credit] is null&#xA;go&#xA;Update DailySales&#xA;SET [EasyPay] = 0&#xA;Where [EasyPay] is null&#xA;go&#xA;Update DailySales&#xA;SET [Synchrony] = 0&#xA;Where [Synchrony] is null&#xA;go&#xA;Update DailySales&#xA;SET [ArFleet] = 0&#xA;Where [ArFleet] is null&#xA;go&#xA;Update DailySales&#xA;SET [ArEmployee] = 0&#xA;Where [ArEmployee] is null&#xA;go&#xA;Update DailySales&#xA;SET [AR] = 0&#xA;Where [AR] is null&#xA;go&#xA;Update DailySales&#xA;SET [Other] = 0&#xA;Where [Other] is null&#xA;go&#xA;Update DailySales&#xA;SET [CarCount] = 0&#xA;Where [CarCount] is null&#xA;go&#xA;/* Set null warranty and parts discounts to 0 */&#xA;Update DailySales&#xA;SET [PartsDiscount] = 0&#xA;Where [PartsDiscount] is null&#xA;go&#xA;Update DailySales&#xA;SET [Warranty] = 0&#xA;Where [Warranty] is null&#xA;go&#xA;&#xA;  /* Part discounts */&#xA;  &#xA;  update [DailySales]&#xA;  Set [DailySales].[PartsDiscount] = [DailySales].[PartsDiscount] + [vw_RODiscountTotals].[PartsDiscountTotal]&#xA;  From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.vw_RODiscountTotals ON dbo.DailySales.Store_id = dbo.vw_RODiscountTotals.shop_id AND dbo.DailySales.DrawerDate = dbo.vw_RODiscountTotals.ClosedDate&#xA;  go&#xA;   /* Remove the NonTaxed Part discounts per Iris*/&#xA;update ds&#xA;  Set ds.[PartsDiscount] = ds.[PartsDiscount] - dt.[PartDiscNonTax]&#xA;  From &#xA;   dbo.DailySales ds INNER JOIN&#xA;dbo.vw_NoTax_DiscountsTotals dt ON ds.Store_id = dt.shop_id AND ds.DrawerDate = dt.ClosedDate&#xA;  go&#xA;   &#xA; /* Insert the Payments for the day */&#xA; /* CASH */&#xA; UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[Cash] = [vw_DailyPayments].[TotalAmt]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] = 'Cash'&#xA;  go&#xA;  /* Add Checks to Cash */&#xA;  UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[Cash] = [DailySales].[Cash] + [vw_DailyPayments].[TotalAmt]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] = 'Check'&#xA;  go&#xA;  /* All CREDIT CARD types*/&#xA; UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[Credit] = [vw_DailyPayments].[TotalAmt]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] ='Credit Card' &#xA;  go&#xA;  &#xA;UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[Credit] = [vw_DailyPayments].[TotalAmt] + [DailySales].[Credit]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] ='Credit Card via Remote Pay' &#xA;  go&#xA;  &#xA;  UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[Credit] = [vw_DailyPayments].[TotalAmt] + [DailySales].[Credit]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] ='Credit Card via Terminal' &#xA;  go&#xA;  &#xA;    /* EasyPay */&#xA; UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[EasyPay] = [vw_DailyPayments].[TotalAmt]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] = 'Other - EasyPay'&#xA;  go&#xA;  &#xA;   /* Synchrony */&#xA; UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[Synchrony] = [vw_DailyPayments].[TotalAmt]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] = 'Other - Synchrony'&#xA;  go&#xA;     /* Fleet */&#xA; UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[ArFleet] = [vw_DailyPayments].[TotalAmt]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] = 'Other - Fleet AR'&#xA;  go&#xA;&#xA;    /* Employee */&#xA; UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[ArEmployee] = [vw_DailyPayments].[TotalAmt]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] = 'Other - Employee AR'&#xA;  go&#xA;&#xA;       /* Other */&#xA; UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[Other] = [vw_DailyPayments].[TotalAmt]&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] = 'Check'&#xA;  go&#xA;&#xA;   /* Warranty */&#xA; UPDATE [dbo].[DailySales]&#xA; SET [DailySales].[warranty] = [vw_DailyPayments].[TotalAmt] * -1&#xA; From &#xA;   dbo.DailySales INNER JOIN&#xA;dbo.[vw_DailyPayments] ON dbo.DailySales.Store_id = [vw_DailyPayments].shop_id AND dbo.DailySales.DrawerDate = [vw_DailyPayments].CreatedDate&#xA;  where dbo.[vw_DailyPayments].[payment_type] = 'Other - Warranty'&#xA;  go&#xA;&#xA;&#xA;   /* Insert the Market */&#xA;    /* Run the copyTables SSIS job to keep this updated */&#xA;    UPDATE [dbo].[DailySales]&#xA; SET [DailySales].Market = [stores].[company]&#xA; From&#xA; dbo.DailySales INNER JOIN&#xA; [Stores] on DailySales.Store_id = [Stores].[StoreNum]&#xA; go&#xA;&#xA;   /* Non-Tax */&#xA;   /* Taxable = 1 or 0 */&#xA;   /*RO_Repair_orders  [DailySales].[ShopSupplies]*/&#xA;   /*Add all up and put in [DailySales].[NonTaxed]*/&#xA;  UPDATE ds&#xA;  SET ds.[NonTaxed] = nt.[nontaxed]&#xA;  FROM            &#xA;  dbo.vw_NoTax_ShopSuppliesTotals nt INNER JOIN&#xA; [DailySales] ds ON nt.shop_id = ds.Store_id AND nt.ClosedDate = ds.DrawerDate&#xA; go&#xA; /* Now Subtract the NonTaxed ShopSupplies sales from the Taxed ShopSupplies Sales per Iris*/ &#xA;  UPDATE ds&#xA;  SET ds.[ShopSupplies] =  ds.[ShopSupplies] - nt.[nontaxed]&#xA;  FROM&#xA;  dbo.vw_NoTax_ShopSuppliesTotals nt INNER JOIN&#xA; [DailySales] ds ON nt.shop_id = ds.Store_id AND nt.ClosedDate = ds.DrawerDate&#xA; go&#xA;  &#xA;  /* RO_Hazmats  [DailySales].[Hazmats]*/&#xA;  UPDATE ds&#xA;  SET ds.[NonTaxed] =  ds.[NonTaxed] + nt.[nontaxed]&#xA;  FROM&#xA;  dbo.vw_NoTax_HazmatsTotals nt INNER JOIN&#xA; [DailySales] ds ON nt.shop_id = ds.Store_id AND nt.ClosedDate = ds.DrawerDate&#xA; go&#xA;  /* Now subtract the NonTaxed Hazmats per Iris */&#xA;update ds&#xA;  Set ds.[NonTaxed] = ds.[NonTaxed] - dt.[NonTaxed]&#xA;  From &#xA;   dbo.DailySales ds INNER JOIN&#xA;vw_NoTax_HazmatsTotals dt ON ds.Store_id = dt.shop_id AND ds.DrawerDate = dt.ClosedDate&#xA;  go&#xA;  /* RO_Labors  [DailySales].[Labor]*/&#xA;   UPDATE ds&#xA;  SET ds.[NonTaxed] =  ds.[NonTaxed] + nt.[nontaxed]&#xA;  FROM&#xA;  dbo.vw_NoTax_LaborsTotals nt INNER JOIN&#xA; [DailySales] ds ON nt.shop_id = ds.Store_id AND nt.ClosedDate = ds.DrawerDate&#xA; go&#xA; /* Now Subtract the NonTaxed LABOR sales from the Taxed LABOR Sales - Added for NMX*/ &#xA;  UPDATE ds&#xA;  SET ds.[labor] =  ds.[labor] - nt.[nontaxed]&#xA;  FROM&#xA;  dbo.vw_NoTax_LaborsTotals nt INNER JOIN&#xA; [DailySales] ds ON nt.shop_id = ds.Store_id AND nt.ClosedDate = ds.DrawerDate&#xA; go&#xA;  /* RO_Parts   [DailySales].[Parts]*/&#xA;   UPDATE ds&#xA;  SET ds.[NonTaxed] =  ds.[NonTaxed] + nt.[nontaxed]&#xA;  FROM&#xA;  dbo.vw_NoTax_PartsTotals nt INNER JOIN&#xA; [DailySales] ds ON nt.shop_id = ds.Store_id AND nt.ClosedDate = ds.DrawerDate&#xA; go&#xA; /* Now Subtract the NonTaxed Part sales from the Taxed Part Sales per Iris*/ &#xA;  UPDATE ds&#xA;  SET ds.[Parts] =  ds.[Parts] - nt.[nontaxed]&#xA;  FROM&#xA;  dbo.vw_NoTax_PartsTotals nt INNER JOIN&#xA; [DailySales] ds ON nt.shop_id = ds.Store_id AND nt.ClosedDate = ds.DrawerDate&#xA; go&#xA;   /* RO_Sublets  [DailySales].[Sublets]*/&#xA; UPDATE ds&#xA;  SET ds.[NonTaxed] =  ds.[NonTaxed] + nt.[nontaxed]&#xA;  FROM&#xA;  dbo.vw_NoTax_SubletTotals nt INNER JOIN&#xA; [DailySales] ds ON nt.shop_id = ds.Store_id AND nt.ClosedDate = ds.DrawerDate&#xA; go&#xA; /* Now subtract the NonTaxed sublets (labor) per Iris */&#xA; UPDATE ds&#xA;  SET ds.[Sublets] =  ds.[Sublets] - nt.[nontaxed]&#xA;  FROM&#xA;  dbo.vw_NoTax_SubletTotals nt INNER JOIN&#xA; [DailySales] ds ON nt.shop_id = ds.Store_id AND nt.ClosedDate = ds.DrawerDate&#xA; go&#xA;  &#xA;  /*Update Car Counts */&#xA;update [DailySales]&#xA;  Set [DailySales].[CarCount] = [vw_CarCount].[CarCount] &#xA;  From&#xA;   dbo.DailySales INNER JOIN&#xA; dbo.vw_CarCount ON dbo.DailySales.Store_id = dbo.vw_CarCount.shop_id &#xA; AND dbo.DailySales.DrawerDate = dbo.vw_CarCount.ClosedDate&#xA;go&#xA; &#xA;/* Set the State and City Taxes */&#xA;   &#xA; /* Update City Tax */&#xA;/* Update Hazmat Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxCity] = ds.[SalesTaxCity] + dt.[TotalCityTax]&#xA;FROM&#xA;vw_DailyHazmatTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;/* Update Labor Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxCity] = ds.[SalesTaxCity] + dt.[TotalCityTax]&#xA;FROM&#xA;vw_DailyLaborTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;/* Update Part Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxCity] = ds.[SalesTaxCity] + dt.[TotalCityTax]&#xA;FROM&#xA;vw_DailyPartTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;/* Update ShopSupplies Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxCity] = ds.[SalesTaxCity] + dt.[TotalCityTax]&#xA;FROM&#xA;vw_DailyShopSuppTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;/* Update Sublet Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxCity] = ds.[SalesTaxCity] + dt.[TotalCityTax]&#xA;FROM&#xA;vw_DailySubletTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;/* Update Tax Discounts  */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxCity] = ds.[SalesTaxCity] + dt.[CityTaxDiscount]&#xA;FROM&#xA;vw_DailyTaxDiscounts dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;  /* Update State Tax */&#xA;/* Update Hazmat Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxState] = ds.[SalesTaxState] + dt.[TotalStateTax]&#xA;FROM&#xA;vw_DailyHazmatTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;/* Update Labor Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxState] = ds.[SalesTaxState] + dt.[TotalStateTax]&#xA;FROM&#xA;vw_DailyLaborTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;/* Update Part Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxState] = ds.[SalesTaxState] + dt.[TotalStateTax]&#xA;FROM&#xA;vw_DailyPartTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;/* Update ShopSupplies Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxState] = ds.[SalesTaxState] + dt.[TotalStateTax]&#xA;FROM&#xA;vw_DailyShopSuppTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;/* Update Sublet Tax */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxState] = ds.[SalesTaxState] + dt.[TotalStateTax]&#xA;FROM&#xA;vw_DailySubletTax dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go&#xA;&#xA;/* Update Tax Discounts  */&#xA;UPDATE ds&#xA;SET ds.[SalesTaxState] = ds.[SalesTaxState] + dt.[StateTaxDiscount]&#xA;FROM&#xA;vw_DailyTaxDiscounts dt INNER JOIN&#xA;[DailySales] ds ON dt.shop_id = ds.Store_id AND dt.ClosedDate = ds.DrawerDate&#xA;go" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
