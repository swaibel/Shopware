      DTS:ObjectName="Add NonTaxed Totals"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C75B70C3-2411-4D23-907B-62FAF8BF3676}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="/*Set values to zero*/&#xA;Update [dbo].[DailyProcess]&#xA; Set [NonTaxed]= 0&#xA;    &#xA; GO&#xA; &#xA; /*Add Non Taxed Hazmats rates Use vw_NoTax_Hazmats*/&#xA;&#xA; Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[NonTaxed]  = [dbo].[vw_NoTax_Hazmats].[NonTaxed] + [dbo].[DailyProcess].[NonTaxed]&#xA; &#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;[dbo].[vw_NoTax_Hazmats] ON  [dbo].[DailyProcess].[RoNumber] = [dbo].[vw_NoTax_Hazmats].[RoNumber]&#xA;&#xA; /*Add Non Taxed Discounts rates Use vw_NoTax_Discounts*/&#xA;    Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[NonTaxed]  = [dbo].[vw_NoTax_Discounts].[LaborDiscountDollars] + [dbo].[vw_NoTax_Discounts].[PartDiscountDollars] + [dbo].[DailyProcess].[NonTaxed]&#xA; &#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;[dbo].[vw_NoTax_Discounts] ON  [dbo].[DailyProcess].[RoNumber] = [dbo].[vw_NoTax_Discounts].[RoNumber]&#xA;&#xA; /*Add Non Taxed Parts rates Use vw_NoTax_Parts*/&#xA; Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[NonTaxed]  = [dbo].[vw_NoTax_Parts].[NonTaxed] + [dbo].[DailyProcess].[NonTaxed]&#xA; &#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;[dbo].[vw_NoTax_Parts] ON  [dbo].[DailyProcess].[RoNumber] = [dbo].[vw_NoTax_Parts].[RoNumber]&#xA; &#xA; /*Add Non Taxed ShopSupplies rates Use vw_NoTax_ShopSupplies*/&#xA; Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[NonTaxed]  = [dbo].[vw_NoTax_ShopSupplies].[NonTaxed] + [dbo].[DailyProcess].[NonTaxed]&#xA; &#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;[dbo].[vw_NoTax_ShopSupplies] ON  [dbo].[DailyProcess].[RoNumber] = [dbo].[vw_NoTax_ShopSupplies].[RoNumber]&#xA;&#xA; /*Add Non Taxed Labors rates Use vw_NoTax_Labors*/&#xA; Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[NonTaxed]  = [dbo].[vw_NoTax_Labors].[NonTaxed] + [dbo].[DailyProcess].[NonTaxed]&#xA; &#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;[dbo].[vw_NoTax_Labors] ON  [dbo].[DailyProcess].[RoNumber] = [dbo].[vw_NoTax_Labors].[RoNumber]&#xA; &#xA; /*Add Non Taxed Sublets rates Use vw_NoTax_Sublets*/&#xA;Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[NonTaxed]  = [dbo].[vw_NoTax_Sublets].[NonTaxed] + [dbo].[DailyProcess].[NonTaxed]&#xA; &#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;[dbo].[vw_NoTax_Sublets] ON  [dbo].[DailyProcess].[RoNumber] = [dbo].[vw_NoTax_Sublets].[RoNumber]&#xA; &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Add Sales Details"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{A85DB45F-8F24-465F-933B-367F523B42BD}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Add Sales Details"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C75B70C3-2411-4D23-907B-62FAF8BF3676}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="/*Set values to zero*/&#xA;Update [dbo].[DailyProcess]&#xA; Set Parts= 0, &#xA; partsDiscount = 0,&#xA; Labor = 0,&#xA; LaborDiscount = 0,&#xA; ShopSupplies =0,&#xA; Hazmats = 0&#xA;GO&#xA;/*Add Sales Detail*/&#xA;Update [dbo].[DailyProcess]&#xA;  SET [dbo].[DailyProcess].[Parts]  = [dbo].[vw_ROTotals].[PartsDollars],&#xA;  [dbo].[DailyProcess].[PartsDiscount]  = [dbo].[vw_ROTotals].[PartDiscountDollars],&#xA;  [dbo].[DailyProcess].[Labor]  = [dbo].[vw_ROTotals].[LaborDollars],&#xA;  [dbo].[DailyProcess].[LaborDiscount]  = [dbo].[vw_ROTotals].[LaborDiscountDollars],&#xA;  [dbo].[DailyProcess].[ShopSupplies]  = [dbo].[vw_ROTotals].[ShopSuppliesDollars],&#xA;  [dbo].[DailyProcess].[Hazmats]  = [dbo].[vw_ROTotals].[HazmatDollars]&#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;dbo.[vw_ROTotals] ON  dbo.DailyProcess.RONumber = dbo.vw_ROTotals.RONumber " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Add Taxes"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{D0FC375F-916C-4970-98E4-497CBD796BC1}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Add Taxes"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C75B70C3-2411-4D23-907B-62FAF8BF3676}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="/*Set values to zero*/&#xA; Update [dbo].[DailyProcess]&#xA; Set SalesTaxTotal = 0, &#xA; SalesTaxStateTotal=0, &#xA; SalesTaxcityTotal = 0,&#xA; TaxHazmats = 0,&#xA; TaxLabor = 0,&#xA; TaxParts = 0,&#xA; TaxShopSupplies = 0&#xA; GO&#xA; &#xA;/*Add Totals Use vw_ROTaxTotals*/&#xA;&#xA;UPDATE DP        &#xA;set DP.SalesTaxTotal = TAX.TotalTax, &#xA;DP.SalesTaxStateTotal = TAX.StateTax, &#xA;DP.SalesTaxCityTotal =TAX.CityTax&#xA;&#xA;FROM dbo.DailyProcess DP &#xA;INNER JOIN&#xA;dbo.vw_ROTaxTotals TAX ON DP.RONumber = TAX.RONumber&#xA;&#xA;GO&#xA;&#xA; /*Add Taxes Hazmats Use vw_ROTaxHazmats*/&#xA; Update [dbo].[DailyProcess]&#xA;  SET&#xA;  [dbo].[DailyProcess].[TaxHazmats] =   [dbo].[vw_ROTaxHazmats].[TotalTax]&#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;dbo.[vw_ROTaxHazmats] ON  dbo.DailyProcess.RONumber = dbo.vw_ROTaxHazmats.RONumber &#xA;&#xA;GO&#xA;&#xA;/*Add Taxes Labor Use vw_ROTaxLaborNet */&#xA; Update DP&#xA;  SET&#xA;  DP.[TaxLabor] =   NET.[TotalTax]&#xA;  FROM [dbo].[DailyProcess] DP&#xA;  INNER JOIN&#xA;dbo.[vw_ROTaxLaborNet] NET ON  DP.RONumber = NET.RONumber &#xA;&#xA;&#xA;GO&#xA;&#xA;/*Add Taxes Labor Use vw_ROTaxPartNet */&#xA; Update DP&#xA;  SET &#xA;  DP.[TaxParts] =   NET.[TotalTax]&#xA;  FROM [dbo].[DailyProcess] DP&#xA;  INNER JOIN&#xA;dbo.[vw_ROTaxPartNet] NET ON  DP.RONumber = NET.RONumber &#xA;&#xA;&#xA;GO&#xA;/*Add Taxes ShopSupplies Use vw_ROTaxShopSupplies*/&#xA;Update [dbo].[DailyProcess]&#xA;  SET&#xA;  [dbo].[DailyProcess].[TaxShopSupplies] = [dbo].[vw_ROTaxShopSupplies].[TotalTax]&#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;dbo.[vw_ROTaxShopSupplies] ON  dbo.DailyProcess.RONumber = dbo.vw_ROTaxShopSupplies.RONumber &#xA;&#xA;&#xA;GO&#xA;/*Add Taxes Discounts Use vw_ROTaxDiscountsTotals  - Now coming from vw_ROTaxLaborNet and vw_ROTaxPartsNet &#xA;Update [dbo].[DailyProcess]&#xA;  SET   [dbo].[DailyProcess].[TaxPartDiscounts] = [dbo].[vw_ROTaxDiscountsTotals].[TotalPartDiscountTax],&#xA;[dbo].[DailyProcess].[TaxLaborDiscounts] = [dbo].[vw_ROTaxDiscountsTotals].[TotalLaborDiscountTax]&#xA;&#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;dbo.[vw_ROTaxDiscountsTotals] ON  dbo.DailyProcess.RONumber = dbo.vw_ROTaxDiscountsTotals.RONumber */&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Add TaxRates and Market"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{2F5A24FE-F4EB-4971-9031-4802055B660D}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Add TaxRates and Market"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C75B70C3-2411-4D23-907B-62FAF8BF3676}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="/*Set values to zero*/&#xA;Update [dbo].[DailyProcess]&#xA; Set [TaxRateTotal]= 0,&#xA;     [TaxRateState] = 0,&#xA;     [TaxRateCity] = 0&#xA; GO&#xA; &#xA; /*Add Tax rates Use Stores*/&#xA; Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[TaxRateState]  = [dbo].[Stores].[StateTaxRate],&#xA;  [dbo].[DailyProcess].[TaxRateCity]  = [dbo].[Stores]. [CityTaxRate],&#xA;  [dbo].[DailyProcess].[TaxRateTotal]  = [dbo].[Stores].[StateTaxRate] + [dbo].[Stores]. [CityTaxRate],&#xA;  [dbo].[DailyProcess].[Market] = [dbo].[Stores].[Company]&#xA;  &#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;dbo.[Stores] ON  dbo.DailyProcess.Store_id = dbo.stores.StoreNum " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Add Total"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{1CE55AAC-A746-45EA-91ED-80E68E75FBBE}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Add Total"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C75B70C3-2411-4D23-907B-62FAF8BF3676}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[Total]  = [dbo].[DailyProcess].[Subtotal] + [dbo].[DailyProcess].[SalesTaxTotal]&#xA;&#xA;GO&#xA;update [dbo].[DailyProcess] &#xA;      SET [Total] = round([Total],2),&#xA;       [SalesTaxTotal] = round([SalesTaxTotal],2),&#xA;       [SalesTaxStateTotal] = round([SalesTaxStateTotal],2),&#xA;       [SalesTaxCityTotal] = round([SalesTaxCityTotal],2)&#xA;  FROM [Shopware].[dbo].[DailyProcess]" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Calculate SubTotal"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{99210245-E3D4-4021-9FA4-BBE4DE4EFDCB}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Calculate SubTotal"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C75B70C3-2411-4D23-907B-62FAF8BF3676}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="&#xA;/*Set values to zero*/&#xA;Update [dbo].[DailyProcess]&#xA; Set subtotal= 0&#xA; GO&#xA;/*Calculate SubTotal*/&#xA;Update [dbo].[DailyProcess]&#xA;  SET [dbo].[DailyProcess].[Subtotal]  =&#xA;  [dbo].[DailyProcess].[Parts] +&#xA;  [dbo].[DailyProcess].[PartsDiscount] + &#xA;  [dbo].[DailyProcess].[Labor]  +&#xA;  [dbo].[DailyProcess].[LaborDiscount]  +&#xA;  [dbo].[DailyProcess].[ShopSupplies]  +&#xA;  [dbo].[DailyProcess].[Hazmats]  " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
