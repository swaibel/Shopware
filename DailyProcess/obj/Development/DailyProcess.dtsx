          SQLTask:SqlStatementSource="/*Set values to zero*/&#xA;Update [dbo].[DailyProcess]&#xA; Set [PaidOnClosedDate]= 0,&#xA;    [TotalPaid]= 0,&#xA;&#x9;[AmtDue]= 0&#xA; GO&#xA; &#xA; /*Add Total Payments On Closed Date Use vw_RoPaymentDailyTotals*/&#xA;&#xA;  Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[PaidOnClosedDate]  = DT.[DailyPaymentTotal]&#xA;  FROM [dbo].[DailyProcess] DP&#xA;  INNER JOIN&#xA;   [dbo].[vw_RoPaymentDailyTotals] DT&#xA;   ON&#xA;  DP.[ClosedDate] = DT.[PaymentDate]   and&#xA;  DP.[RONumber] =  DT.[RONumber]&#xA;  GO&#xA;   /*Add Total Payments All Dates Use [vw_ROPaymentTotals]*/&#xA;   &#xA;  Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[TotalPaid]  = [dbo].vw_ROPaymentTotals.[PaymentDollars] &#xA; &#xA;  FROM [dbo].[DailyProcess]&#xA;  INNER JOIN&#xA;[dbo].vw_ROPaymentTotals ON  [dbo].[DailyProcess].[RoNumber] = [dbo].vw_ROPaymentTotals.[RoNumber]&#xA;GO&#xA;&#xA;/*Calculate if any amount due */&#xA;&#xA;Update [dbo].[DailyProcess]&#xA;  SET &#xA;  [dbo].[DailyProcess].[AmtDue]  = [dbo].[DailyProcess].[Total] - [dbo].[DailyProcess].[TotalPaid]&#xA;GO&#xA;&#xA;&#xA;/*Fix rounding errors */&#xA;update [dbo].[DailyProcess]&#xA;Set amtdue = 0  &#xA;where amtdue = .01 &#xA;GO&#xA;&#xA;update [dbo].[DailyProcess]&#xA;Set amtdue = 0  &#xA;where amtdue = -.01 &#xA;GO&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
